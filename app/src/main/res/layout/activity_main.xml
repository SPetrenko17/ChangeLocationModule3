<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding ="10pt"
    android:id="@+id/main"
    android:orientation="vertical">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/SettingsFrame">

        <fragment android:name="com.example.sergei.changelocationmodule3.SettingsFragment"
            android:id="@+id/tfragmentSettings"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </fragment>
    </FrameLayout>


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ListFrame"
        android:layout_below="@+id/SettingsFrame">
        <fragment
            android:id="@+id/listfragment"
            android:name="com.example.sergei.changelocationmodule3.ListFragmentListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
        </fragment>
    </FrameLayout>



   </LinearLayout>


